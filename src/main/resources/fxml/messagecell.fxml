<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<VBox fx:id="vBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text fx:id="content" strokeType="OUTSIDE" strokeWidth="0.0" text="Content">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Text>
      <Text fx:id="date" strokeType="OUTSIDE" strokeWidth="0.0" text="Date">
         <font>
            <Font size="10.0" />
         </font>
      </Text>
   </children>
</VBox>
